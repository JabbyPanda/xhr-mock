{
  "name": "@xhr-mock/router",
  "version": "1.0.0",
  "description": "Router for handling requests and responses.",
  "keywords": [
    "mock",
    "xhr",
    "fetch",
    "router"
  ],
  "repository": "jameslnewell/xhr-mock",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "dependencies": {
    "@types/statuses": "^1.5.0",
    "global": "^4.3.2",
    "path-to-regexp": "^3.0.0",
    "statuses": "^1.5.0",
    "url": "^0.11.0"
  },
  "scripts": {
    "clean": "rm -rf ./lib ./dist",
    "check:linting:sources": "tslint --config tslint.sources.json --project tsconfig.sources.json",
    "check:linting:tests": "tslint --config tslint.tests.json --project tsconfig.tests.json",
    "check:typing:sources": "tsc --noEmit --project tsconfig.sources.json",
    "check:typing:tests": "tsc --noEmit --project tsconfig.tests.json",
    "check": "yarn run check:linting:sources && yarn run check:linting:tests && yarn run check:typing:sources && yarn run check:typing:tests",
    "build:cjs": "tsc --pretty --declaration",
    "build": "yarn run build:cjs",
    "test": "jest"
  },
  "license": "MIT"
}
